{
  "name": "@kodadot1/static",
  "version": "0.0.8-rc.0",
  "description": "Static constants in the KodaDot development",
  "repository": "kodadot/nft-gallery",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
