    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,css,scss,md}": [
      "prettier --write",
      "yarn lint:quiet"
    ]
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "prettier": {
    "semi": false,
    "bracketSameLine": true
