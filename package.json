    "@polkadot/vue-identicon": "^3.6.2",
    "@ramp-network/ramp-instant-sdk": "^4.0.4",
    "@transak/transak-sdk": "^1.4.1",
    "@types/node": "^20.8.8",
    "@vitejs/plugin-vue": "^4.4.0",
    "apollo-boost": "^0.4.9",
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^2.0.1",
    "chartjs-plugin-annotation": "^2.2.1",
    "chartjs-plugin-zoom": "^1.2.1",
    "date-fns": "^2.30.0",
    "emoji-unicode": "^2.0.1",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.2",
    "highlight.js": "^11.9.0",
    "keen-slider": "^6.8.6",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "mingo": "^5.1.0",
    "nuxt": "^3.8.0",
    "ofetch": "^1.3.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qrcode.vue": "^3.4.1",
    "rollup": ">=2.0.0 <3.0.0",
    "setimmediate": "^1.0.5",
    "slugify": "^1.6.6",
    "unzipit": "^1.4.3",
    "vite": "*",
    "vue-apollo": "^3.1.1",
