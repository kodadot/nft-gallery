    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "fake-indexeddb": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
