  "name": "nft-gallery-nuxt",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev-docker": "npx nuxt",
    "dev-experimental": "nuxt --hostname localhost --port 9090",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.vue .",
